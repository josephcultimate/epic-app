<ol class="breadcrumb">
    <li>{{#link-to "index" id="tenant_tenants_navButton"}}Tenants{{/link-to}}</li>
    <li id="tenant_alias_listItem" class="active">{{alias}}</li>
</ol>
<form class="form-horizontal" role="form">
    <div class="form-group">
        <label id="tenant_id_label" class="col-sm-2 control-label">Id</label>

        <div class="col-sm-10">
            <p id="tenant_id_value" class="form-control-static">{{id}}</p>
        </div>
    </div>
    <div class="form-group">
        <label id="tenant_name_label" class="col-sm-2 control-label">Name</label>

        <div class="col-sm-10">
            <p id="tenant_name_value" class="form-control-static">{{name}}</p>
        </div>
    </div>
    <div class="form-group">
        <label id="tenant_alias_label" class="col-sm-2 control-label">Alias</label>

        <div class="col-sm-10">
            <p id="tenant_alias_value" class="form-control-static">{{alias}}</p>
        </div>
    </div>
    <div class="form-group">
        <label id="tenant_createdAt_label" class="col-sm-2 control-label">Created At</label>

        <div class="col-sm-10">
            <p id="tenant_createdAt_value" class="form-control-static">{{created_at}}</p>
        </div>
    </div>
    <div class="form-group">
        <label id="tenant_enabled_label" class="col-sm-2 control-label">Enabled</label>

        <div class="col-sm-10">
            <p id="tenant_enabled_value" class="form-control-static">{{enabled}}
                {{#if enabled}}
                    <a id="tenant_disableTenant_button" class="btn btn-link btn-sm" {{action 'disable' this}}><strong>(Disable)</strong></a>
                {{else}}
                    <a id="tenant_enableTenant_button" class="btn btn-link btn-sm" {{action 'enable' this}}><strong>(Enable)</strong></a>
                {{/if}}
            </p>
        </div>
    </div>
    <div class="form-group">
        <label id="tenant_preferredLocale_label" class="col-sm-2 control-label">Preferred Locale</label>

        <div class="col-sm-10">
            <p id="tenant_preferredLocale_value" class="form-control-static">{{preferred_locale}}</p>
        </div>
    </div>
</form>
{{#if errorMessage}}
    <div id="tenant_errorMessage_div" class="alert alert-error">{{errorMessage}}</div>
{{/if}}